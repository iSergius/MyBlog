<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <changeSet id="1" author="Kondratyev Sergey" context="it">
        <insert tableName="article_t">
            <column name="article_id" value="1"/>
            <column name="article_title" value="My First Article"/>
            <column name="article_published" value="TRUE"/>
        </insert>
        <insert tableName="marker_t">
            <column name="marker_id" value="1"/>
            <column name="marker_title" value="News"/>
        </insert>
        <insert tableName="article_marker_t">
            <column name="article_marker_id" value="1"/>
            <column name="article_id" value="1"/>
            <column name="marker_id" value="1"/>
        </insert>
    </changeSet>

    <changeSet id="2" author="Kondratyev Sergey" context="clear">
        <!--<rollback changeSetId="1"/>-->
        <delete tableName="article_t"/>
        <delete tableName="market_t"/>
        <delete tableName="article_marker_t"/>
    </changeSet>

    <include file="/src/test/resources/sql/postgresql/content.sql"/>

</databaseChangeLog>